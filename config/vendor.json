[
  {
    "from": [
      "node_modules/bootstrap/dist/css/bootstrap.css",
      "node_modules/@fortawesome/fontawesome-free/css/fontawesome.css"
    ],
    "to": "assets/css/vendor.css"
  },

  { 
    "from": "node_modules/@fortawesome/fontawesome-free/webfonts/*.{woff2,eot}", 
    "to": "assets/fonts/fontawesome"
  },

  {
    "from": "node_modules/roboto-fontface/fonts/roboto/roboto-light.woff2",
    "to": "assets/fonts/roboto"
  },

  {
    "from": "node_modules/hogan.js/dist/hogan-3.0.2.js",
    "to": "assets/js/compiler.js"
  },

  {
    "from": "node_modules/jerc-utils/src/JMain/js/jmain.js",
    "to": "assets/js"
  },

  {
    "from": "node_modules/jerc-utils/src/JMain/js/workers/*.js",
    "to": "assets/js/workers"
  },
  
  {
    "from": "node_modules/jerc-utils/src/JMain/service-worker.js",
    "to": ""
  }
]