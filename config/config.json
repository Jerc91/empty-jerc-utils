{
  "packages": {
    "name": "vendor",
    "path": "./config/vendor.json"
  },
  "paths": {
    "dev": "src/",
    "prod": "dist/",
    "libs": "assets/",
    "filesUpdate": "assets/config/filesToUpdate.json",
    "serviceWorker": "service-worker.js",
    "requesterTools": "assets/js/workers/requester-tools.js"
  },
  "types": {
    "css": "**/*.css",
    "fonts": "**/*.{ttf,svg,woff,woff2,eot,otf}",
    "html": "**/*.html",
    "img": "**/*.{jpg,png,gif,ico}",
    "pug": "**/*.{jade,pug}",
    "js": "**/*.js",
    "json": "**/*.json",
    "less": "**/*.less",
    "sass": "**/*.{scss,sass}"
  },
  "excludeWatchs": [
    "**/filesToUpdate.json",
    "**/service-worker.js",
    "**/requester-tools.js"
  ],
  "server": {
    "sslKey": "./config/ssl/server.key",
    "sslCrt": "./config/ssl/server.crt",
    "httpPort": 4000,
    "httpsPort": 443,
    "portLivereload": 444,
    "urlApp": "127.0.0.1"
  },
  "sass": [
    "main.scss"
  ],
  "cache": {
    "version": "0.0.61",
    "date": "05/12/2018, 11:45:26 am",
    "files": [
      "assets/fonts/fontawesome/fa-brands-400.eot",
      "assets/fonts/fontawesome/fa-brands-400.woff2",
      "assets/fonts/fontawesome/fa-regular-400.eot",
      "assets/fonts/fontawesome/fa-regular-400.woff2",
      "assets/fonts/fontawesome/fa-solid-900.eot",
      "assets/fonts/fontawesome/fa-solid-900.woff2",
      "assets/fonts/roboto/Roboto-Light.woff2",
      "assets/js/jmain.js",
      "assets/js/workers/requester-tools.js",
      "assets/js/workers/requester-worker.js",
      "favicon.ico",
      "index.html",
      "scripts/app.js",
      "service-worker.js"
    ]
  },
  "scriptServer": "node app"
}